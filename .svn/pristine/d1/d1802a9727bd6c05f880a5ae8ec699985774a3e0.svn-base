<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/siri_head_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/my_touxiang"
        android:layout_marginLeft="10dp"
        />

    <com.liuzhuni.lzn.core.goods.ui.LinearLayoutForListView
        android:id="@+id/siri_goods_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/duihuakuang_bg_system"
        android:layout_toRightOf="@id/siri_head_iv"
        android:layout_marginTop="18dp"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="10dp"
        android:orientation="vertical"
        >
    </com.liuzhuni.lzn.core.goods.ui.LinearLayoutForListView>



</RelativeLayout>