<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@drawable/loginout_dialog"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/text_content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="20sp"
        android:textColor="@color/me_info_text"
        android:singleLine="true"
        android:text="@string/loginout_dialog"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/regist_text"

        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/loginout_submit"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="20sp"
            android:textColor="@color/me_info_text"
            android:text="@string/submit_dialog"
            android:gravity="center"

            />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/regist_text"
            />
        <TextView
            android:id="@+id/loginout_cancel"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="20sp"
            android:textColor="@color/key"
            android:text="@string/cancel_dialog"
            android:gravity="center"
            />



        </LinearLayout>



</LinearLayout>
